{
  "name": "exence",
  "version": "1.0.1",
  "description": "A framework providing all the essential tools to the JS coder.",
  "main": "src/essence.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "dependencies": {
    "ursa": "^0.9.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.12.2",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-import": "^2.2.0",
    "http-server": "^0.9.0",
    "webpack": "^1.14.0"
  },
  "scripts": {
    "babel": "babel --presets es2015 src/essence.js -o build/essence.bundle.js",
    "csslint": "csslint src",
    "eslint": "eslint src",
    "start": "http-server",
    "test": "eslint src & csslint src",
    "webpack": "webpack",
    "doc": "jsdoc -c ./conf.json",
